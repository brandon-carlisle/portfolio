---
const items = [
  { name: "Home", pathname: "/" },
  { name: "About", pathname: "/about" },
  { name: "Projects", pathname: "/projects" },
];
---

<nav class="border border-amber-100">
  <h2>
    <a href="/">Brandon Carlisle</a>
  </h2>

  <ul>
    {
      items.map((item) => (
        <li>
          <a href={item.pathname}>{item.name}</a>
        </li>
      ))
    }
  </ul>
</nav>
